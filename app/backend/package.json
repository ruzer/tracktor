{
  "name": "backend",
  "version": "0.0.1",
  "description": "Tractor backend",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc && tsc-alias",
    "dev": "tsx --watch index.ts",
    "start": "node dist/index.js",
    "preview": "npm run build && npm run start",
    "db:migrate": "tsx src/db/migrate.ts migrate",
    "db:seed": "tsx src/db/migrate.ts seed",
    "db:status": "tsx src/db/status.ts",
    "format": "(eslint --fix . || true) && prettier --write .",
    "lint": "eslint . && prettier --check .",
    "clean": "rm -rf dist && rm -rf tracktor.db",
    "reset": "npm run clean && rm -rf tracktor.db node_modules && npm install"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^6.0.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.0",
    "express": "^4.21.2",
    "express-rate-limit": "^8.0.1",
    "sequelize": "^6.37.7",
    "sqlite3": "^5.1.7"
  },
  "devDependencies": {
    "@eslint/js": "^9.34.0",
    "@faker-js/faker": "^10.0.0",
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.21",
    "@types/node": "^24.0.13",
    "@types/sqlite3": "^3.1.11",
    "@typescript-eslint/eslint-plugin": "^8.40.0",
    "@typescript-eslint/parser": "^8.40.0",
    "eslint": "^9.34.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-unused-imports": "^4.2.0",
    "prettier": "^3.6.2",
    "tsc-alias": "^1.8.16",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3"
  }
}
